   <string notr="true">/* --- Global Styles --- */
QWidget {
    font-family: &quot;Segoe UI&quot;, Arial, sans-serif;
 font-size: 10pt;
    color: #333;
}

QMainWindow {
    background-color: #f0f2f5;
 }

/* --- Navigation Sidebar --- */
#navFrame {
    background-color: #ffffff;
    border-right: 1px solid #e0e0e0;
 }

#navFrame QPushButton {
    text-align: left;
    padding: 12px 20px;
    border: none;
    border-radius: 8px;
    background-color: transparent;
    font-weight: 500;
 }

#navFrame QPushButton:hover {
    background-color: #f0f2f5;
}

#navFrame QPushButton:checked {
    background-color: #e6f2ff;
    color: #005a9e;
    font-weight: bold;
 }

#headerLabel {
    font-size: 14pt;
    font-weight: bold;
    color: #005a9e;
    padding: 10px;
 }


/* --- Content Area --- */
#page_employees, #page_attendance, #page_projects, #page_leave_approval, #page_departments {
    background-color: #f0f2f5;
 }

QGroupBox {
    font-weight: bold;
    background-color: #ffffff;
    border: 1px solid #dcdcdc;
    border-radius: 8px;
    margin-top: 10px;
 }
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    padding: 0 10px;
    left: 10px;
 }

/* --- General Widgets (ĐÃ SỬA) --- */
QLineEdit, QComboBox, QDateEdit, QDoubleSpinBox {
    border: 1px solid #dcdcdc;
 border-radius: 8px;
    padding: 8px 12px;
    background-color: white;
}
QLineEdit:focus, QComboBox:focus, QDateEdit:focus, QDoubleSpinBox:focus {
    border: 1px solid #0078d7;
 }

/* Style cho mũi tên của QDateEdit */
QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
 border-left-width: 1px;
    border-left-color: #dcdcdc;
    border-left-style: solid;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}
QDateEdit::down-arrow {
    image: url(:/icons/calendar.svg);
 /* Giả sử bạn có icon này */
    width: 14px;
    height: 14px;
 }

/* Style cho mũi tên của QDoubleSpinBox (MỚI) */
QDoubleSpinBox::up-button, QDoubleSpinBox::down-button {
    subcontrol-origin: padding;
    width: 20px;
    border-left-width: 1px;
 border-left-color: #dcdcdc;
    border-left-style: solid;
}
QDoubleSpinBox::up-button {
    subcontrol-position: top right;
    border-top-right-radius: 8px;
 }
QDoubleSpinBox::down-button {
    subcontrol-position: bottom right;
    border-bottom-right-radius: 8px;
}
QDoubleSpinBox::up-arrow {
    width: 10px;
    height: 10px;
 /* (Sử dụng mũi tên mặc định của hệ thống) */
}
QDoubleSpinBox::down-arrow {
    width: 10px;
    height: 10px;
 /* (Sử dụng mũi tên mặc định của hệ thống) */
}


QPushButton {
    padding: 8px 18px;
 border: 1px solid #dcdcdc;
    border-radius: 8px;
    background-color: white;
    min-width: 80px;
}
QPushButton:hover {
    background-color: #f5f5f5;
    border-color: #c0c0c0;
 }
QPushButton:pressed {
    background-color: #e0e0e0;
}


/* --- Specialized Buttons --- */

/* --- Blue Buttons (ĐÃ SỬA) --- */
QPushButton#btnAdd, QPushButton#btnAddProject {
    background-color: #0078d7;
 color: white;
    border-color: #0078d7;
    font-weight: bold;
}
QPushButton#btnAdd:hover, QPushButton#btnAddProject:hover {
    background-color: #005a9e;
    border-color: #004578;
 }

/* --- Red Buttons (ĐÃ SỬA) --- */
QPushButton#btnDelete, QPushButton#btnDeleteProject, QPushButton#btnRejectLeave {
    background-color: #d9534f;
    color: white;
    border-color: #d9534f;
 }
QPushButton#btnDelete:hover, QPushButton#btnDeleteProject:hover, QPushButton#btnRejectLeave:hover {
    background-color: #c9302c;
    border-color: #ac2925;
 }

/* --- Green Buttons (MỚI) --- */
QPushButton#btnSaveChanges, QPushButton#btnCompleteProject, QPushButton#btnApproveLeave {
    background-color: #28a745;
 /* Green */
    color: white;
    border-color: #28a745;
    font-weight: bold;
 }
QPushButton#btnSaveChanges:hover, QPushButton#btnCompleteProject:hover, QPushButton#btnApproveLeave:hover {
    background-color: #218838;
    border-color: #1e7e34;
 }


/* --- Table --- */
QTableWidget {
    border: 1px solid #dcdcdc;
    border-radius: 8px;
    gridline-color: #e0e0e0;
    background-color: white;
 }
QHeaderView::section {
    background-color: #f8f9fa;
    padding: 10px;
    border: none;
    border-bottom: 1px solid #dcdcdc;
    font-weight: bold;
 }
QTableWidget::item {
    padding: 10px;
}
QTableWidget::item:selected {
    background-color: #cfe2ff;
    color: #000;
 }

/* --- ScrollBar --- */
QScrollBar:vertical {
    border: none;
    background: #f0f2f5;
    width: 10px;
    margin: 0px 0px 0px 0px;
 }
QScrollBar::handle:vertical {
    background: #c0c0c0;
    min-height: 20px;
    border-radius: 5px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
 }</string>
